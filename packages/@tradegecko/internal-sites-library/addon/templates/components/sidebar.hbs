<aside class="">
  <img src="/logo.svg" width="210" height="50">
  <p class="sub-text">Our Brand</p>

  {{#each-in this.navWithChildren as | index nav |}}
    {{log nav}}
    <h4 class="sidebar-title">
      {{nav.title}}
    </h4>
    <ul>
      {{#each nav.pages as |child|}}
        <SidebarItem @url={{child.url}} @title={{child.title}} />
      {{/each}}
      {{#each-in nav.children as |index childNav|}}
        <SidebarFolder @title={{child.title}} @pages={{child.pages}} />
      {{/each-in}}
    </ul>
  {{/each-in}}
  <!--<p><br><br></p>
  <a class="button">Download PDF</a>
  <p><br><br></p>-->
</aside>
